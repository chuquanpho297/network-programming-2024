<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="roomTableView" layoutY="9.0" maxHeight="600.0" minHeight="600.0" prefHeight="600.0" prefWidth="1032.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <columns>
        <TableColumn fx:id="itemIdColumn" prefWidth="150.0" text="Item ID" />
        <TableColumn fx:id="itemNameColumn" prefWidth="150.0" text="Item Name" />
        <TableColumn fx:id="buyNowPriceColumn" text="Buy Now Price" />
        <TableColumn fx:id="timestampColumn" text="Updated At" />
        <TableColumn fx:id="stateColumn" text="State" />
        <TableColumn fx:id="actionColumn" text="Actions" />

    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>
